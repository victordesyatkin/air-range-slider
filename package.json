{
  "name": "air-range-slider",
  "version": "1.0.10",
  "description": "jQuery range slider",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:css": "node-sass src/ -o dist/ && postcss dist/*.css --use autoprefixer postcss-preset-env cssnano -d dist/",
    "build:css:dev": "node-sass src/ -o dist/ && postcss dist/*.css --use autoprefixer postcss-preset-env -d dist/",
    "build": "webpack --env mode=production && npm run build:css",
    "build:dev": "webpack --env mode=development && npm run build:css:dev",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --fix --ext .ts, src/",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "homepage": "https://victordesyatkin.github.io/slider/index.html",
  "author": "https://github.com/victordesyatkin",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victordesyatkin/air-range-slider"
  },
  "keywords": [
    "range-slider",
    "plugin",
    "customizable",
    "powerful"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.13",
    "@prettier/plugin-pug": "^1.13.5",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.18",
    "@types/jquery": "^3.5.4",
    "@types/lodash": "^4.14.165",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.0",
    "autoprefixer": "^10.2.5",
    "clean-webpack-plugin": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-fsd": "^1.0.1",
    "eslint-plugin-i18n-json": "^3.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jsdom": "^16.4.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@types/lodash.isundefined": "^3.0.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.orderby": "^4.6.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/lodash.uniq": "^4.5.6",
    "bind-decorator": "^1.0.11",
    "classnames": "^2.2.6",
    "lodash.isundefined": "^3.0.1",
    "lodash.merge": "^4.6.2",
    "lodash.orderby": "^4.6.0",
